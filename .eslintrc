{
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "extends": [
    "eslint:recommended",
  ],
  "parser": "@babel/eslint-parser",
  "rules": {
    "no-console": 0,
    "max-len": 0,
    "strict": 0,
    "import/no-dynamic-require": 0,
    "import/newline-after-import": 0,
    "no-underscore-dangle": 0,
    "consistent-return": 0,
    "import/no-extraneous-dependencies": 0,
    "no-plusplus": 0,
    "no-useless-escape": 0,
    "no-unneeded-ternary": 0,
    "class-methods-use-this": 0,
    "no-param-reassign": 0,
    "comma-dangle": 0
  }
}
